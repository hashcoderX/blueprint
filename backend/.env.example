# Backend environment variables (Express + MySQL)
# Copy to `.env` and adjust per environment.

# Server
PORT=3001
JWT_SECRET=replace-with-a-secure-random-string

# MySQL connection
DB_HOST=localhost
DB_USER=blueprint_app
DB_PASSWORD=Bp_AppPass123
DB_NAME=blueprint

# Google OAuth (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
# In production, point to your domain callback
GOOGLE_REDIRECT_URI=https://your-domain.com/auth/google/callback
